{
  "branches": ["main"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "npm run build && git stash -u && git fetch origin release:release || git checkout -b release && git checkout release && git stash pop && git add dist/ && git commit -m 'chore: update build output [skip ci]' && git push origin release && git checkout - && git stash pop"
      }
    ],
    "@semantic-release/github"
  ],
  "tagFormat": "v${version}"
}