{
  "branches": ["main"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "git checkout -B release main && pnpm run build && git add dist && git commit -m 'chore: update build output' || true && git push -f origin release"
      }
    ]
  ],
  "tagFormat": "v${version}"
}